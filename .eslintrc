{
  "parser": "babel-eslint",
  "extends": [
    "standard"
  ],
  "plugins": [
    "jest",
    "class-property"
  ],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "no-console": [
      "warn"
    ],
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "jest/valid-expect": "error"
  }
}
